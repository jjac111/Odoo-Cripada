<odoo>
    <data>
	
		<record id="stock_picking_form_new" model="ir.ui.view">
            <field name="name">stock.picking.form.new</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='owner_id']" position="after">
                    <field name="cliente_final" widget="many2one"/>
                </xpath>
				
				<xpath expr="//field[@name='owner_id']" position="after">
                    <field name="helpdesk_ticket" widget="many2one"/>
                </xpath>

            </field>
        </record>
		
		
		<record id="product_template_product_form_new" model="ir.ui.view">
            <field name="name">product.template.product.form.new</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='name']" position="after">
                    <field name="owner_id" widget="many2one"/>
                </xpath>
					

            </field>
        </record>

    </data>
</odoo>